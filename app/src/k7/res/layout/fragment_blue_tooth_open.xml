<?xml version="1.0" encoding="utf-8"?>
<com.mili.smarthome.tkj.main.widget.SetSuccessView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/title_back">


    <TextView
        android:layout_marginTop="@dimen/qb_px_30"
        android:id="@+id/tv_title"
        style="@style/SettingTitleStyle"
        android:layout_alignParentTop="true" />

    <LinearLayout
        android:id="@+id/lin_input"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/qb_px_30"
        android:gravity="center"
        android:orientation="vertical">


            <RelativeLayout
                android:id="@+id/rl_pwd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <TextView
                    android:id="@+id/tv_register_id"
                    style="@style/SettingTextViewStyle"
                    android:layout_alignParentStart="true"
                    android:gravity="center"
                    android:text="@string/setting_device_register" />


                <com.mili.smarthome.tkj.set.widget.inputview.CustomInputView
                    android:id="@+id/it_register_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/tv_register_id"
                    android:layout_centerVertical="true"
                    >

                </com.mili.smarthome.tkj.set.widget.inputview.CustomInputView>

            </RelativeLayout>

    </LinearLayout>



</com.mili.smarthome.tkj.main.widget.SetSuccessView>