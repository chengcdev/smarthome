<?xml version="1.0" encoding="utf-8"?>
<com.mili.smarthome.tkj.main.widget.SetSuccessView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_title"
        style="@style/SettingTitleStyle"
        android:layout_alignParentTop="true"
        />

    <RelativeLayout
        android:id="@+id/rl_show"
        android:layout_centerVertical="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_name"
            style="@style/SettingLeftTvStyle"
             />

        <com.mili.smarthome.tkj.set.widget.inputview.CustomInputView
            android:id="@+id/it_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/tv_name">

        </com.mili.smarthome.tkj.set.widget.inputview.CustomInputView>

    </RelativeLayout>


</com.mili.smarthome.tkj.main.widget.SetSuccessView>